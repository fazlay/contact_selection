<?xml version="1.0" encoding="utf-8"?>
<odoo>


   <record id="view_contact_type_list" model="ir.ui.view">
          <field name="name">contact.type.list</field>
          <field name="model">contact.selection.type</field>
              <field name="type">tree</field>
          <field name="arch" type="xml"> 
             <tree string="Contact Types">
                <field name="name"/>
                <field name="key"/>
            </tree>
          </field>
   </record>


    <record id="action_contact_type" model="ir.actions.act_window">
        <field name="name">Contact Ypes</field>
        <field name="res_model">contact.selection.type</field>
       <field name="view_mode">tree,form</field> 
    <field name="view_id" ref="view_contact_type_list"/> 
        <field name="help" type="html">
                <p class="oe_view_nocontent_smiling_face">
                    Click to Create New Patient Tag
                </p>
            </field>
    </record>

      
        <menuitem id="menu_partner_contact_type_form"
        action="action_contact_type"
        name="Contact Types"
        sequence="1" parent="contacts.res_partner_menu_config"/>

</odoo>